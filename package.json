{
  "name": "react-simple-focus-within",
  "version": "0.0.0-development",
  "description": "The missing JS API of the CSS `:focus-within` for React",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "source": "index.js",
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "NODE_ENV=cjs rollup -c -o dist/index.js",
    "build:esm": "NODE_ENV=esm rollup -c -o dist/index.m.js",
    "prepublishOnly": "npm run build",
    "pretest": "npm run lint",
    "test": "echo \"Error: no test specified\" && exit 0",
    "lint": "eslint .",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "start": "styleguidist server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okonet/react-simple-focus-within.git"
  },
  "keywords": [],
  "author": "Andrey Okonetchnikov <andrey@okonet.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/okonet/react-simple-focus-within/issues"
  },
  "homepage": "https://github.com/okonet/react-simple-focus-within#readme",
  "dependencies": {
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.3.0",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.14.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-styleguidist": "^7.2.2",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "styled-components": "^3.4.2",
    "webpack": "^4.16.5",
    "webpack-blocks": "^1.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "react": ">=16.0",
    "react-dom": ">=16.0"
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  }
}
